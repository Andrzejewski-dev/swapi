{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "http-server": {
      "type": "library",
      "root": "libs/http-server",
      "entryFile": "index",
      "sourceRoot": "libs/http-server/src",
      "compilerOptions": {
        "tsConfigPath": "libs/http-server/tsconfig.lib.json"
      }
    },
    "swagger": {
      "type": "library",
      "root": "libs/swagger",
      "entryFile": "index",
      "sourceRoot": "libs/swagger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/swagger/tsconfig.lib.json"
      }
    },
    "cors": {
      "type": "library",
      "root": "libs/cors",
      "entryFile": "index",
      "sourceRoot": "libs/cors/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cors/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "swapi": {
      "type": "library",
      "root": "libs/swapi",
      "entryFile": "index",
      "sourceRoot": "libs/swapi/src",
      "compilerOptions": {
        "tsConfigPath": "libs/swapi/tsconfig.lib.json"
      }
    }
  }
}