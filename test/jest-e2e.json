{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "swapi/http-server/(.*)": "<rootDir>/../libs/http-server/src/$1",
    "swapi/http-server": "<rootDir>/../libs/http-server/src",
    "swapi/swagger/(.*)": "<rootDir>/../libs/swagger/src/$1",
    "swapi/swagger": "<rootDir>/../libs/swagger/src",
    "@swapi/cors/(.*)": "<rootDir>/../libs/cors/src/$1",
    "@swapi/cors": "<rootDir>/../libs/cors/src",
    "@swapi/logger/(.*)": "<rootDir>/../libs/logger/src/$1",
    "@swapi/logger": "<rootDir>/../libs/logger/src"
  }
}